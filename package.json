{
  "name": "trinkwasser-check",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:capacitor": "cross-env NEXT_FONT_IGNORE_ERRORS=1 next build",
    "build:capacitor:static": "cross-env NEXT_FONT_IGNORE_ERRORS=1 CAPACITOR_BUILD=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "seed:openfoodfacts": "node scripts/seed-openfoodfacts.js",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npm run build:capacitor && npx cap sync",
    "cap:sync:android": "npm run build:capacitor && npx cap sync android",
    "cap:sync:android:static": "npm run build:capacitor:static && npx cap sync android",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android --no-sync",
    "cap:build:android": "npm run cap:sync:android:static && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@prisma/client": "^6.19.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "@zxing/browser": "^0.1.5",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.553.0",
    "next": "^14.2.16",
    "next-pwa": "^5.6.0",
    "prisma": "^6.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tesseract.js": "^6.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.16",
    "happy-dom": "^20.0.10",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vitest": "^4.0.9"
  }
}
